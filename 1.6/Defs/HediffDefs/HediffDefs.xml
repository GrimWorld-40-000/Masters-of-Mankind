<Defs>

	<HediffDef>
		<defName>GW_Ruler_ValdorSpearXpDrain</defName>
		<label>valdor's spear</label>
		<description>WIP.</description>
		<isBad>false</isBad>
		<hediffClass>HediffWithComps</hediffClass>
		<initialSeverity>0.001</initialSeverity>
		<comps>
		  <li Class="XpDrain.HediffCompProperties_XpOnKill">
			<isDrainAtRandom>false</isDrainAtRandom>
			<drainPercent>0.05</drainPercent>
		  </li>
		</comps>
	</HediffDef>

	<HediffDef>
		<defName>GW_Ruler_EmperorAura</defName>
		<label>emperor's aura</label>
		<description>WIP.</description>
		<isBad>false</isBad>
		<hediffClass>HediffWithComps</hediffClass>
		<initialSeverity>0.001</initialSeverity>
		<renderNodeProperties>
			<li Class="PawnRenderNodeProperties">
				<texPath>Emperor/Headgear/GW_HRM_Emperor_LaurelsAura</texPath>
				<parentTagDef>ApparelHead</parentTagDef>
				<drawSize>1.2</drawSize>
				<useGraphic>true</useGraphic>
				<shaderTypeDef>MoteGlow</shaderTypeDef>
				<baseLayer>100</baseLayer>
				<pawnType>Any</pawnType>
				<drawData>
					<defaultData>
						<layer>20</layer>
						<offset>(0, 0, 0)</offset>
					</defaultData>
					<dataWest>
						<flip>false</flip>
					</dataWest>
					<dataNorth>
						<layer>75</layer>						
					</dataNorth>
				</drawData>
			</li>
		</renderNodeProperties>
    <abilities>
      <li>EmperorLOTDAbility</li>
    </abilities>
		<comps>
			<li Class="gwrulers.HediffCompProperties_AbilitiesStartWithCooldown" />
			<li Class="GW40kHediffAura.HediffCompProperties_AuraAndMote">
				<ownerFactionHediff>GW40k_SS_InspiringAura_Reciever</ownerFactionHediff>
				<affectWearer>false</affectWearer>
				<severityPerTrigger>1</severityPerTrigger>
				<tickInterval>120</tickInterval>
				<radius>5</radius>
				<mote>GW40k_SS_Aura</mote>
				<uiIconEnabled>UI/Icon-PowerGuilliman-Active</uiIconEnabled>
				<uiIconDisabled>UI/Icon-PowerGuilliman-Inactive</uiIconDisabled>
			</li>
		</comps>
		<stages>
			<li>
				<minSeverity>1</minSeverity>
			</li>
		</stages>
	</HediffDef>

	<HediffDef>
		<defName>GW_Ruler_ValdorShieldBonus</defName>
		<label>valdor's shield</label>
		<description>WIP.</description>
		<isBad>false</isBad>
		<hediffClass>HediffWithComps</hediffClass>
		<initialSeverity>0.001</initialSeverity>
		<comps>
		  <li Class="VEF.Hediffs.HediffCompProperties_Shield">
                <compClass>VEF.Hediffs.HediffComp_Shield</compClass>
                <graphic>
                    <texPath>Other/ShieldBubble</texPath>
                    <drawSize>(2, 2)</drawSize>
                    <graphicClass>Graphic_Single</graphicClass>
                    <shaderType>MoteGlow</shaderType>
                </graphic>
				<maxEnergy>100</maxEnergy>
				<energyLossPerDamage>0.033</energyLossPerDamage>
				<energyPerTick>0.1</energyPerTick>
				<rechargeDelay>60</rechargeDelay>
                <absorbAttackType>Both</absorbAttackType>
                <sustainer>BulletShield_Ambience</sustainer>
                <soundEnded>EnergyShield_Broken</soundEnded>
                <doDust>false</doDust>
                <absorbedFleck>ExplosionFlash</absorbedFleck>
                <damageOnAttack>Melee</damageOnAttack>
                <damageType>Flame</damageType>
                <damageAmount>3</damageAmount>
            </li>
		</comps>
	</HediffDef>

	<HediffDef>
		<defName>GW_Ruler_MalcadorBonus</defName>
