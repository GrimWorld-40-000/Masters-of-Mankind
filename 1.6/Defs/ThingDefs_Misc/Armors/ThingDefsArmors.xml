<Defs>
	
	
	<ThingDef Abstract="true" Name="GW_Rulers_ArmorBase" ParentName="ArmorMachineableBase">
		<techLevel>Spacer</techLevel>
		<recipeMaker>
			<unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
			<skillRequirements>
				<Crafting>20</Crafting>
				<Intellectual>15</Intellectual>
			</skillRequirements>
			<recipeUsers Inherit="false">
				<li>GW_Autoforge</li>
			</recipeUsers>
		</recipeMaker>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>1</drawSize>
		</graphicData>
		<thingCategories>
			<li>ApparelArmor</li>
		</thingCategories>
		<uiIconScale>1</uiIconScale>
		<equippedStatOffsets>
			<Beauty>50</Beauty>			
			<DecompressionResistance MayRequire="kentington.saveourship2">0.75</DecompressionResistance>
			<VacuumSpeedMultiplier MayRequire="kentington.saveourship2">4</VacuumSpeedMultiplier>
			<VacuumResistance MayRequire="ludeon.rimworld.odyssey">0.6</VacuumResistance>
		</equippedStatOffsets>
		<apparel>
			<drawData>
				<dataNorth>
					<offset>(0.0, 0.0, 0.0)</offset>
				</dataNorth>
				<dataWest>
					<offset>(0.0, 0.0, -0.1)</offset>
				</dataWest>
				<dataEast>
					<offset>(0.0, 0.0, -0.1)</offset>
				</dataEast>
				<!--<scale>1.5</scale> Uncomment these to increase size while equipped
                <childScale>1.5</childScale>-->
			</drawData>
			<tags>
				<li>Spacer</li>
				<li>EVA</li>
				<li>PrestigeCombatGear</li>
				<li>RoyalTier7</li>
			</tags>
			<useWornGraphicMask>true</useWornGraphicMask>
			<countsAsClothingForNudity>true</countsAsClothingForNudity>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Neck</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
				<li MayRequire="ceteam.combatextended">Hands</li>
				<li MayRequire="ceteam.combatextended">Feet</li>
			</bodyPartGroups>
			<layers>
				<li>Middle</li>
				<li>Shell</li>
			</layers>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
		</apparel>
		<comps>
			<li MayRequire="kentington.saveourship2">
				<compClass>SaveOurShip2.CompEVA</compClass>
			</li>
		</comps>
		<modExtensions>
			<li Class="VEF.Apparels.ApparelExtension">
				<isUnifiedApparel>true</isUnifiedApparel>
				<hideHead>false</hideHead>
			</li>
			<li Class="CombatExtended.PartialArmorExt" MayRequire="ceteam.combatextended">
				<stats>
					<li>
						<ArmorRating_Sharp>0.8</ArmorRating_Sharp>
						<parts>
							<li>Hand</li>
						</parts>
					</li>
					<li>
						<ArmorRating_Blunt>0.8</ArmorRating_Blunt>
						<parts>
							<li>Hand</li>
						</parts>
					</li>
				</stats>
			</li>
		</modExtensions>
	</ThingDef>
	
